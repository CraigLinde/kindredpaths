<% if current_user %>
    <%if current_user.role == "Admin"%>
    <body class = "art_page">
    <div class = "margin5">
    <nav><%=render partial: 'shared/navlinks'%></nav>
    <h1>Users</h1>
    </div>
    <div class="row">
    <div class = "col-md-2"></div>
    <div class = "col-md-8">
    <table class="table">
        <thead>
            <tr>
            <th scope="col" class="BG_head">email</th>
            <th scope="col" class="BG_head">name</th>
            <th scope="col" class="BG_head">current role</th>
            <th scope="col" colspan="2"></th>
            </tr>
        </thead>

        <tbody>
        <% @users.each do |user| %>
        <tr>
            <td class="BG_List"><%= user.email%></td>
            <td class="BG_List"><%= user.email %></td>
            <td class="BG_List"><%= user.role %></td>
        </tr>
        <% end %>
        </tbody>
    </table>
    <%end%>
<%else%>
<h1>You Are Not Logged In</h1>
<%end%>
</body>
</div>